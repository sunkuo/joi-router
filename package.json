    "joi"
  ],
  "author": "sunkuo <sunkuo@chuxin.ren>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sunkuo/joi-router/issues"
  },
  "homepage": "https://github.com/sunkuo/joi-router#readme",
  "devDependencies": {
    "coveralls": "^2.13.1",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.13.3",
    "istanbul": "^0.4.5",
    "mocha": "^3.3.0",
    "power-assert": "^1.4.2",
    "supertest": "^3.0.0",
    "validate-commit-msg": "^2.12.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {}
  },
  "dependencies": {
